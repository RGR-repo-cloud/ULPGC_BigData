<?xml version="1.0" encoding="UTF-8"?>
<hazelcast xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.hazelcast.com/schema/config
           http://www.hazelcast.com/schema/config/hazelcast-config-5.6.xsd">

    <cluster-name>dev</cluster-name>

    <network>
        <port auto-increment="true" port-count="10">5701</port>
        
        <join>
            <multicast enabled="false"/>
            <tcp-ip enabled="true" connection-timeout-seconds="3">
                <member-list>
                    <member>127.0.0.1:5701</member>
                    <member>127.0.0.1:5702</member>
                </member-list>
            </tcp-ip>
        </join>
    </network>
    
    <properties>
        <property name="hazelcast.max.join.seconds">10</property>
    </properties>
    
</hazelcast>
